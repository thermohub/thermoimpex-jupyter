      "members": [
        {
          "name": "CONSTANT_T",
          "value": 0,
          "doc": "Constant on T\n"
        },
        {
          "name": "ARRHENIUS_ABS",
          "value": 1,
          "doc": "Arrhenius equation evaluated at absolute temperature\n"
        },
        {
          "name": "ARRHENIUS_DIF",
          "value": 2,
          "doc": "Arrhenius equation evaluated at temperature relative to Tref\n"
        },
        {
          "name": "OTHER_T",
          "value": 101,
          "doc": "Other methods of rate constant correction on T\n"
        }
      ]
