            "lowerT": 273.15,
            "upperT": 298.15,
            "lowerP": 1,
            "upperP": 1000000
          }
        },
        {
          "key": 27,
          "name": "m_heat_capacity_p_t_coeffs",
          "typeId": "struct",
          "type": {
            "typeId": "struct",
            "class": "prop.Item"
          },
          "doc": "Coefficients of Cp0(T) function per mole \"CpTc\"\n",
          "required": "optional",
          "default": {
            "name": "CpTc"
          }
        },
        {
          "key": 28,
