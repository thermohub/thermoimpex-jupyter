{
  "name": "ingredient",
  "doc": "Apache Thrift IDL definition for the ingredient service interface\n",
  "namespaces": {
    "*": "ingredient"
  },
  "includes": [
"prop",
"element",
"substance",
"constituent"
  ],
  "enums": [
  ],
  "typedefs": [
  ],
  "structs": [
    {
      "name": "Ingredient",
      "doc": "Description of ingredient = aggregated compositions (e.g. rocks from real minerals)\n",
      "isException": false,
      "isUnion": false,
      "fields": [
        {
          "key": 1,
          "name": "symbol",
          "typeId": "string",
          "doc": "Ingredient symbol e.g. \"Solid\" or \"Aqueous\"\n",
          "required": "required"
        },
        {
          "key": 2,
          "name": "comment",
          "typeId": "string",
          "doc": "Name comment of the ingredient (e.g. \"Portland cement clinker powder\")\n",
          "required": "req_out"
        },
        {
          "key": 3,
          "name": "composition",
          "typeId": "double",
          "doc": "Composition contribution of this ingredient to the whole recipe\n",
          "required": "req_out",
          "default": 0
        },
        {
          "key": 4,
          "name": "unitM",
          "typeId": "string",
          "doc": "Units\n",
          "required": "req_out",
          "default": "mol"
        },
        {
          "key": 5,
          "name": "density",
          "typeId": "double",
          "doc": "Volume or density\n",
          "required": "req_out",
          "default": 0
        },
        {
          "key": 6,
          "name": "unitV",
          "typeId": "string",
          "doc": "Units\n",
          "required": "req_out",
          "default": "kg\/m^3"
        },
        {
          "key": 7,
          "name": "reactionExtent",
          "typeId": "double",
          "doc": "Extent of reaction, i.e. mass fraction of ingredient composition to equilibrate\n",
          "required": "req_out",
          "default": 1
        },
        {
          "key": 11,
          "name": "constituents",
          "typeId": "map",
          "type": {
            "typeId": "map",
            "keyTypeId": "string",
            "valueTypeId": "struct",
            "valueType": {
              "typeId": "struct",
              "class": "constituent.Constituent"
            }
          },
          "doc": "List of constituents (solid, gaseous, aqueous)\n",
          "required": "req_out"
        },
        {
          "key": 12,
          "name": "formulae",
          "typeId": "map",
          "type": {
            "typeId": "map",
            "keyTypeId": "string",
            "valueTypeId": "struct",
            "valueType": {
              "typeId": "struct",
              "class": "prop.FormulaData"
            }
          },
          "doc": "Shortcut: List of composing formulae e.g. Fe | Fe2O3 | Fe+2 , optional\n",
          "required": "req_out"
        },
        {
          "key": 13,
          "name": "scalval",
          "typeId": "struct",
          "type": {
            "typeId": "struct",
            "class": "prop.ScalingValues"
          },
          "doc": "Scaling values at the whole ingredient level\n",
          "required": "req_out"
        },
        {
          "key": 21,
          "name": "ibcObjectComputed",
          "typeId": "bool",
          "doc": "Indicator of calculation consistency\n",
          "required": "req_out",
          "default": 0
        },
        {
          "key": 22,
          "name": "ibcObject",
          "typeId": "map",
          "type": {
            "typeId": "map",
            "keyTypeId": "string",
            "valueTypeId": "double"
          },
          "doc": "List of elements and their masses for this ingredient\n",
          "required": "req_out",
          "default": {
            "O": 0
          }
        },
        {
          "key": 23,
          "name": "ibcMass",
          "typeId": "double",
          "doc": "Mass of this ibcObject in kg\/mol (calculated using the elements list)\n",
          "required": "req_out",
          "default": 0
        },
        {
          "key": 25,
          "name": "timeStamp",
          "typeId": "struct",
          "type": {
            "typeId": "struct",
            "class": "prop.TimeStamp"
          },
          "doc": "Timestamp\n",
          "required": "optional"
        }
      ]
    },
    {
      "name": "Ingredients",
      "doc": "Definition of an array of ingredients\n",
      "isException": false,
      "isUnion": false,
      "fields": [
        {
          "key": 2,
          "name": "ingredients",
          "typeId": "list",
          "type": {
            "typeId": "list",
            "elemTypeId": "struct",
            "elemType": {
              "typeId": "struct",
              "class": "Ingredient"
            }
          },
          "doc": "List of chemical composition ingredients\n",
          "required": "req_out"
        }
      ]
    }
  ],
  "constants": [
  ],
  "services": [
  ]
}
